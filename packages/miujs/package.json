{
  "name": "miujs",
  "description": "Simple and fast build tool for website",
  "version": "0.1.1",
  "author": "mast1ff",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miujs/miujs.git",
    "directory": "packages/miujs"
  },
  "bugs": {
    "url": "https://github.com/miujs/miujs/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/miujs/miujs/tree/main/#readme",
  "license": "MIT",
  "bin": {
    "miu": "bin/miu.js"
  },
  "main": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "files": [
    "bin",
    "dist",
    "node.js",
    "node.d.ts",
    "browser.js",
    "browser.d.ts",
    "connect.js",
    "connect.d.ts",
    "express.js",
    "experss.d.ts",
    "vercel.js",
    "vercel.d.ts",
    "netlify.js",
    "netlify.d.ts"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "clean": "rimraf dist node_modules",
    "deps": "npx depcheck",
    "dev:node": "rollup -c -w",
    "dev": "run-p dev:*",
    "build:node": "rimraf dist && rollup -c",
    "build": "run-p build:*",
    "typecheck": "tsc --noEmit",
    "test": "jest --silent=false --verbose false --updateSnapshot",
    "test:watch": "jest --silent=false --verbose false --updateSnapshot --watchAll",
    "lint": "eslint --ext .ts 'src/**'",
    "format": "prettier --write --parser typescript \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/busboy": "^0.3.1",
    "@types/compression": "^1.7.2",
    "@types/connect": "^3.4.35",
    "@types/cookie": "^0.4.1",
    "@types/cookie-signature": "^1.0.4",
    "@types/dashify": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^16.2.14",
    "@types/jsesc": "^3.0.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/morgan": "^1.9.3",
    "@types/node-fetch": "^2.6.1",
    "@types/path-to-regexp": "^1.7.0",
    "@types/serve-static": "^1.13.10",
    "@types/source-map-support": "^0.5.4",
    "@types/ws": "^8.5.3",
    "intersection-observer": "^0.12.0",
    "rollup": "^2.70.2"
  },
  "dependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@netlify/functions": "^1.0.0",
    "@riotjs/compiler": "^6.1.3",
    "@riotjs/ssr": "^8.0.0",
    "@vercel/node": "^1.14.1",
    "@web-std/file": "^3.0.2",
    "abort-controller": "^3.0.0",
    "autoprefixer": "^10.4.4",
    "busboy": "^0.3.1",
    "cac": "^6.7.12",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "cookie": "^0.5.0",
    "cookie-signature": "^1.2.0",
    "dashify": "^2.0.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.27",
    "esm": "^3.2.25",
    "exit-hook": "^2.2.1",
    "express": "^4.17.3",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.1",
    "get-port": "^5.1.1",
    "jsdom": "^19.0.0",
    "jsesc": "^3.0.2",
    "liquidjs": "^9.36.0",
    "lodash.debounce": "^4.0.8",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.7",
    "path-to-regexp": "^6.2.0",
    "postcss": "^8.4.12",
    "pretty-ms": "^7.0.1",
    "riot": "^6.1.2",
    "sass": "^1.50.1",
    "serve-static": "^1.15.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.3.1",
    "ws": "^8.5.0"
  }
}
